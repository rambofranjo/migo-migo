<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.0 Strict//EN"
"http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd">

<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="de" lang="de">
	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    	<title><% response.title %></title>
    
		<meta name="author-personal" content="David Davorin Barudzija" />
		<meta name="description" content="Management Interface of Game Organization" />
     	<meta name="language" content="de" />
		<meta name="robots" content="index, follow" />
    	
		<!-- CSS -->
		<link rel="stylesheet" href="/static/css/fullcalendar.css" type="text/css" />
		<link rel="stylesheet" href="/static/css/jquery-ui-1.8.17.custom.css" type="text/css">
		<link rel="stylesheet" href="/static/css/style/main.css" type="text/css"  />
		
		<!-- Javascript -->
		<script src="/static/scripts/jquery-1.5.2.min.js"></script>
		<script src="/static/scripts/fullcalendar.js"></script>
		<script src="/static/scripts/jquery-ui-1.8.17.custom.min.js"></script>
		
		<script>
			$(function() {
				$( "#datepicker" ).datepicker({
                    altField: "#datePicked",
					altFormat: "DD, d MM, yy"	
				});
			});
		</script>
		
		
		<script type="text/javascript">
			function showHideCommentsNews(div) {
				var d = '#' + div;

				if (document.getElementById(div).style.display == "none") {
					$(d).show("normal");
				} else {
					$(d).hide("normal");
				}
			}
		</script>
		
		<script type="text/javascript">
			$(document).ready(function() {
		    	// page is now ready, initialize the calendar...

		    	$('#calendar').fullCalendar({
		        	// put your options and callbacks here
		    	})

			});
		</script>
	
		<script type ="text/javascript">
			$(function() {
				var availableTags = [
					<% response.names %>
				];
				$( ".names" ).autocomplete({
					source: availableTags
				});
			});
		</script>
    
</head>

<body class="bg">

	<!-- Wrapper start -->
	<div id="wrapper">

		<div id="header">
			<div id="header_top">
				<div id="header_top_content">
					<p class="right">
						<a href="#">Kundenbereich</a>
						<span style="color:#FFF">|</span>
						<a href="#">Service &amp; Support</a>
					</p>

						<p class="left">
							<strong style="font-size:16px">Migo + Logo</strong>
						</p>
				</div>
			</div>	
		</div>
		
		<div id="content">
			<div id="content_box" style="margin-top:20px">
				
				<!-- +++++++++++++++++++++++ -->
				<!-- Statusleiste -->
				<!-- +++++++++++++++++++++++ -->
				<div id="content_box_txt">
					<% response.loginMsg %>
					<p>Gruppe: <% response.groupName %></p>
					<p><% response.allGroups %></p>
					<div class="clear"></div>
				</div>
			</div>
		
			<!-- +++++++++++++++++++++++ -->
			<!-- Menu -->
			<!-- +++++++++++++++++++++++ -->
			<div id="content_box" style="margin-top:20px">
				<% response.menu %>
			</div>
			
			<div id="content_box" style="margin-top:20px; width:500px; float:left">
				<!-- +++++++++++++++++++++++ -->
				<!-- News -->
				<!-- +++++++++++++++++++++++ -->
				<!-- Neue News schreiben -->
				<% response.newNews %>
		
				<!-- Neue News bearbeiten -->
				<% response.editNews %>
		
				<!-- Alle News auflisten -->
				<% response.listNews %>
				
				<!-- +++++++++++++++++++++++ -->
				<!-- Appointments -->
				<!-- +++++++++++++++++++++++ -->
				<!-- Neue Appointment schreiben -->
				<% response.newAppointment %>
		
				
				<% response.outputAppointment %>
				
				
			</div>
				
		
			<!-- +++++++++++++++++++++++ -->
			<!-- Calendar -->
			<!-- +++++++++++++++++++++++ -->
			<!-- Kalender anzeigen -->
			<!--<div id="<% response.calendar %>" style="width:670px; margin-bottom:10px;"></div> -->
		
		
			<!-- +++++++++++++++++++++++ -->
			<!-- Messages -->
			<!-- +++++++++++++++++++++++ -->
			<!-- Neue News schreiben -->
			<% response.newMessage %>
		
			<!-- Alle Nachrichten auflisten -->
			<% response.listMessages %>
		
		
			<!-- +++++++++++++++++++++++ -->
			<!-- Mitglieder -->
			<!-- +++++++++++++++++++++++ -->
			<!-- Alle Mitglieder auflisten -->
			<% response.listUsers %>
		
			<!-- +++++++++++++++++++++++ -->
			<!-- Gruppeninformationen -->
			<!-- +++++++++++++++++++++++ -->
			<% response.groupInfo %>
			
			<div id="dropbox" style="float:left; width=22%; margin:20px 0px 0px 20px; padding:10px; border:1px dashed #DDDDDD; background-color:#F5F5F5;">
				<% response.dropbox %>
			</div>
			
		</div>
	</div>
	
</body>
</html>